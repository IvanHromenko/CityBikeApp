@using CityBikeApp.Models.Entities;
@model List<Station>

<div class="container">
    <div class="row pt-4 pb-3">
        <div class="col-6">
            <h2 class="text-primary">Station List</h2>
        </div>
        <div class="col-6 text-end">
            <a asp-controller="Station" asp-action="Create" class="btn btn-primary">
                <i class="bi bi-plus-circle"></i> Add new station
            </a>
        </div>
    </div>
</div>
<table id="myTable" class="table table-bordered table-striped">
    <thead>
    <tr>
        <th>
            Station Name
        </th>
            
        <th></th>
    </tr>
    </thead>
    
</table>

@section Scripts{
    <script src="~/js/station.js"></script>
}